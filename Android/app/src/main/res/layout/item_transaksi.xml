<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_marginVertical="10dp"
        android:layout_marginTop="15dp"
        android:weightSum="4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/text_view_no"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:text="No"
            android:layout_width="80dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_view_tracking"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:text="Tracking"
            android:layout_width="80dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_view_franchisee"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:text="Franchisee"
            android:layout_width="200dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_view_nama"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:text="Nama"
            android:layout_width="200dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_view_tanggal"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:text="Date"
            android:layout_width="100dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_view_status"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:text="Status"
            android:layout_width="150dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/text_view_biaya"
            android:textAlignment="textEnd"
            android:textColor="@color/colorBlack"
            android:text="Biaya"
            android:layout_width="150dp"
            android:layout_height="wrap_content"/>
    </LinearLayout>
</TableRow>